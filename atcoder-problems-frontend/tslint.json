{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "indent": [true, "spaces", 2],
    "max-line-length": [true, { "limit":  80 }],
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "interface-name": false,
    "array-type": false
  }
}